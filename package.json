{
  "name": "react-sample-app",
  "version": "1.0.0",
  "description": "React sample application",
  "main": "lib/server.js",
  "repository": "git@github.com:swapnilvjoshi/react-sample-app.git",
  "author": "Swapnil Joshi <swapnil.v.joshi@gmail.com>",
  "license": "MIT",
  "scripts": {
    "checktypes": "tsc",
    "lint": "yarn eslint ./lib/*",
    "start": "tsc && webpack && nodemon --exec babel-node --extensions .ts ./lib/server.ts",
    "webpack": "webpack -wd",
    "test": "jest --watch"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "babel-loader": "^8.0.6",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@types/express": "^4.17.2",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/webpack": "^4.41.5",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2"
  }
}
